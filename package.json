{
  "name": "to-get-there",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "bundle:proto": "ts-node tools/bundle.ts",
    "deploy:libs": "nx run-many --target=build --projects=config,grpc --parallel",
    "deploy:apps": "nx run-many --target=deploy --projects=api-gateway,user-service --parallel",
    "dev:deploy:apps": "nx run-many --target=bootstrap:dev --projects=api-gateway --parallel",
    "deploy:backend": "yarn bundle:proto && yarn deploy:libs && yarn deploy:apps && COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 docker-compose -f deploy/docker-compose.prod.yml -p to-get-there --env-file=envs/.env.prod up",
    "dev:deploy:backend": "COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 docker-compose -f deploy/docker-compose.dev.yml -p to-get-there-dev --env-file=envs/.env.dev up",
    "bootstrap:backend": "yarn dev:deploy:backend",
    "bootstrap:mobile": "cd apps/mobile-app && mkdir -p dev-clients && yarn && yarn build:devclient:ios-sim && (lsof -ti:8081 | xargs kill) && yarn start"
  },
  "private": true,
  "dependencies": {
    "@grpc/grpc-js": "1.8.16",
    "@grpc/proto-loader": "0.7.7",
    "@nestjs/common": "9.4.3",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "9.4.3",
    "@nestjs/microservices": "10.0.3",
    "@nestjs/platform-express": "9.0.0",
    "@nestjs/swagger": "7.0.6",
    "@nestjs/typeorm": "10.0.0",
    "axios": "1.0.0",
    "guards": "link:@nestjs/core/guards",
    "http-proxy-middleware": "2.0.6",
    "next": "13.1.1",
    "pg": "8.11.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.0.0",
    "styled-components": "5.3.6",
    "tslib": "2.3.0",
    "typeorm": "0.3.17"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/cypress": "15.9.2",
    "@nrwl/detox": "15.9.2",
    "@nrwl/eslint-plugin-nx": "15.9.2",
    "@nrwl/jest": "15.9.2",
    "@nrwl/js": "15.9.2",
    "@nrwl/linter": "15.9.2",
    "@nrwl/nest": "15.9.2",
    "@nrwl/next": "15.9.2",
    "@nrwl/node": "15.9.2",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/webpack": "15.9.2",
    "@nrwl/workspace": "15.9.2",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/jest-native": "5.4.2",
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.2",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "babel-jest": "^29.4.1",
    "babel-plugin-styled-components": "1.10.7",
    "cypress": "^12.2.0",
    "detox": "^20.9.0",
    "eas-cli": "~3.13.3",
    "eslint": "~8.15.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "jest": "^29.4.1",
    "jest-circus": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "metro": "0.74.1",
    "metro-resolver": "0.74.1",
    "nx": "15.9.2",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  }
}
